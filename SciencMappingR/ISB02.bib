@article{ ISI:000315976800002,
Author = {Kolkowska, Ella and Dhillon, Gurpreet},
Title = {{Organizational power and information security rule compliance}},
Journal = {{COMPUTERS \& SECURITY}},
Year = {{2013}},
Volume = {{33}},
Number = {{SI}},
Pages = {{3-11}},
Month = {{MAR}},
Abstract = {{This paper analyzes power relationships and the resulting failure in
   complying with information security rules. It argues that an inability
   to understand the intricate power relationships in the design and
   implementation of information security rules leads to a lack of
   compliance with the intended policy. The argument is conducted through
   an empirical, qualitative case study set in a Swedish Social Services
   organization. Our findings indicate that various dimensions of power and
   how these relate to information security rules ensure adequate
   compliance. This also helps to improve configuration of security rules
   through proactive information security management. (C) 2012 Elsevier
   Ltd. All rights reserved.}},
