@inproceedings{ ISI:000241630900023,
Author = {Kwon, Jeong Ok and Jeong, Ik Rae and Lee, Dong Hoon},
Editor = {{Yoshiura, H and Sakurai, K and Rannenberg, K and Murayama, Y and Kawamura, S}},
Title = {{Provably-secure two-round password-authenticated group key exchange in
   the standard model}},
Booktitle = {{ADVANCES IN INFORMATION AND COMPUTER SECURITY, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
