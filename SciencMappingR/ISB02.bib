@article{ ISI:000318245700019,
Author = {Jeong, Yoon-Su and Kim, Yong-Tae and Shin, Seung-Soo and Lee, Sang-Ho},
Title = {{A Device Authentication Mechanism Reducing Performance Cost in Mobile
   P2P Networks}},
Journal = {{KSII TRANSACTIONS ON INTERNET AND INFORMATION SYSTEMS}},
