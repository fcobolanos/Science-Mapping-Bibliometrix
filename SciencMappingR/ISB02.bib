@article{ ISI:000255083200014,
Author = {Boukerche, Azzedine and Oliveira, Horacio A. B. F. and Nakamura, Eduardo
   F. and Loureiro, Antonio A. F.},
Title = {{Secure localization algorithms for wireless sensor networks}},
Journal = {{IEEE COMMUNICATIONS MAGAZINE}},
Year = {{2008}},
Volume = {{46}},
Number = {{4}},
Pages = {{96-101}},
Month = {{APR}},
Abstract = {{In the military and emergency preparedness class of applications,
