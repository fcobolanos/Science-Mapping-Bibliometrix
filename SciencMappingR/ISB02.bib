@article{ ISI:000312973900006,
Author = {Harding, Christopher and Banach-Gutierrez, Joanna Beata},
Title = {{The Emergent EU Criminal Policy: Identifying the Species}},
Journal = {{EUROPEAN LAW REVIEW}},
Year = {{2012}},
Volume = {{37}},
Number = {{6}},
Pages = {{758-770}},
Month = {{DEC}},
Abstract = {{This article analyses the European Commission's Communication of
   September 2011, ``Towards an EU Criminal Policy{''}, in an attempt to
   identify more precisely the nature and role of what is now commonly
   referred to as ``EU criminal law{''} and to extrapolate some theoretical
   foundation for the deployment of criminal law at the European level. The
   analysis is framed around three major tensions in contemporary criminal
   law development-criminalisation versus soft compliance, security versus
   justice and rights protection, and globalisation versus local
   diversity-leading in turn to three main questions: why use criminal law,
   with what risks, and why at EU level?}},
