@article{ ISI:000241966900018,
Author = {Jolly, Lindsay},
Title = {{Will ethanol destabilise the world sugar market?}},
Journal = {{INTERNATIONAL SUGAR JOURNAL}},
Year = {{2006}},
Volume = {{108}},
Number = {{1295}},
Pages = {{606+}},
Month = {{NOV}},
Abstract = {{More and more countries are looking to Brazil as a plausible model for
   developing an integrated sugar and ethanol industry in the expectations
   that demand for renewable transport fuels will expand rapidly, as
   governments increasingly implement energy and environmental policies to
   deal with climate change and compliance with Kyoto Protocol commitments,
   urban air pollution, and energy security. Crucially, rocketing crude oil
   prices since 2004 have further heightened interest in fuel ethanol,
   making the biofuel markedly more competitive as against gasoline than
   ever before. An emerging interface between ethanol and the sugar market
   has seemingly become a key new strategic driver for the world sugar
   market. Whilst the dynamics and potency of this interface are still to
   be fully understood, in part being driven by future oil price levels and
   the extent to which sugar producing industries around the globe
   diversify into fuel ethanol production, a key question is could ethanol
   begin to destabilise the world sugar market over the longer term. The
   implications of Brazil's rapidly expanding flex-fuel vehicle fleet is a
   central focus because it is leading to a profound change in Brazil's
   ability to alter its ethanol/sugar product mix. The risk of a large
   diversion of sucrose away from crystal sugar production to ethanol
   production is assessed in 6 other key countries including the United
   States, the EU, Japan, China, India and Thailand.}},
