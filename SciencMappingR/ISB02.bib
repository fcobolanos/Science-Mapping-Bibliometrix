@article{ ISI:000232484800024,
Author = {Theoharidou, M and Kokolakis, S and Karyda, M and Kiountouzis, E},
Title = {{The insider threat to information systems and the effectiveness of
   ISO17799}},
Journal = {{COMPUTERS \& SECURITY}},
Year = {{2005}},
Volume = {{24}},
Number = {{6}},
Pages = {{472-484}},
Month = {{SEP}},
Abstract = {{Insider threat is widely recognised as an issue of utmost importance for
   IS security management. In this paper, we investigate the approach
   followed by IS017799, the dominant standard in IS security management,
   in addressing this type of threat. We unfold the criminology theory that
   has designated the measures against insider misuse suggested by the
   standard, i.e. the General Deterrence Theory, and explore the possible
   enhancements to the standard that could result from the study of more
   recent criminology theories. The paper concludes with supporting the
   argument for a muttiparadigm and multidisciplinary approach towards IS
   security management and insider threat mitigation. (C) 2005 Elsevier
   Ltd. All rights reserved.}},
