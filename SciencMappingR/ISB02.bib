@article{ ISI:000313100100128,
Author = {Tang Hongyuan and Liao Ni and Liu Chang and Wei Min},
Title = {{Appraisal and Strengthening for a Factory Building subjected to Fire
   Disaster}},
Journal = {{DISASTER ADVANCES}},
Year = {{2012}},
Volume = {{5}},
Number = {{4}},
Pages = {{783-787}},
Month = {{OCT}},
Abstract = {{The test methods and contents for a factory building subjected to fire
   disaster are introduced, including section geometry size, concrete
   strength, steel strength, carbonation depth in concrete and so on. The
   fire damaged structure is security appraised according to the test
   results and calculation analysis. The adapted strengthening methods are
   put forward through the specific appraisal results. The damaged columns
   are respectively strengthened with reinforced concrete, externally
   bonded steel frame and fiber reinforced polymer. For the damaged beams,
   demolition replacement, externally bonded steel frame and fiber
   reinforced polymer are adapted respectively; as the damaged slabs, the
   demolition replacement, bottom bonded rebar with jet concrete and
   externally bonded fiber reinforced polymer are adapted. The structure
   completely resumes its bearing capacity after strengthening.}},
