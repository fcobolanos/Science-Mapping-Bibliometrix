@article{ ISI:000309687800001,
Author = {Thomas, Manoj and Dhillon, Gurpreet},
Title = {{Interpreting Deep Structures of Information Systems Security}},
Journal = {{COMPUTER JOURNAL}},
Year = {{2012}},
Volume = {{55}},
Number = {{10}},
Pages = {{1148-1156}},
Month = {{OCT}},
Abstract = {{Confidentiality, integrity and availability, while being key
   requirements for ensuring security, have had limitations in providing an
   integral security model that incorporates the benefits of all
   predominant designs. Heeding to the call that security models cater to
   unique operational characteristics of individual firms, this paper
   presents a deep structure framework of information systems (ISs)
   security. The framework incorporates three models-representational
   model, which identifies subsystems within an organization;
   state-tracking model, which ensures that different states trace IS
   security in the real world;nd the decomposition model, which defines
   specific external events that are a stimuli to changes in internal
   events. In a final synthesis, the paper presents a deep structure-based
   conceptual clarity that manifests the meaning of dynamic, custom-fit and
   flexible IS security solutions.}},
