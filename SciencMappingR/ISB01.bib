@article{ ISI:000375029000009,
Author = {Hansen, Susanne Therese},
Title = {{Taking ambiguity seriously: Explaining the indeterminacy of the European
   Union conventional arms export control regime}},
Journal = {{EUROPEAN JOURNAL OF INTERNATIONAL RELATIONS}},
