@article{ ISI:000377348000003,
Author = {Bruno, Valentina and Cornaggia, Jess and Cornaggia, Kimberly J.},
Title = {{Does Regulatory Certification Affect the Information Content of Credit
   Ratings?}},
Journal = {{MANAGEMENT SCIENCE}},
Year = {{2016}},
Volume = {{62}},
Number = {{6}},
Pages = {{1578-1597}},
Month = {{JUN}},
Abstract = {{We exploit an investor-paid rating agency's designation as a Nationally
   Recognized Statistical Rating Organization (NRSRO) to test whether this
   certification affects the agency's information production. We use a
   certified issuer-paid agency as a benchmark and find robust evidence
   that the investor-paid agency's ratings policy-both timelier and more
   symmetric with respect to positive and negative information-persists
   after it became certified for regulatory compliance. Our results suggest
   that ratings policy is more a function of rating agency compensation
   structure than the NRSRO certification by the U.S. Securities and
   Exchange Commission.}},
