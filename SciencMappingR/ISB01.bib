@article{ ISI:000322415700006,
Author = {Moody, Gregory D. and Siponen, Mikko},
Title = {{Using the theory of interpersonal behavior to explain non-work-related
   personal use of the Internet at work}},
Journal = {{INFORMATION \& MANAGEMENT}},
Year = {{2013}},
Volume = {{50}},
Number = {{6}},
Pages = {{322-335}},
Month = {{SEP}},
Abstract = {{Non-work-related personal use of the Internet within organizations has
   received increased attention from scholars. We increase previous
   understanding of this phenomenon by proposing a novel model based on the
   theory of interpersonal behavior (TIB). The TIB includes previous
   researched constructs (i.e., attitudes, social influence, and
   intentions) as well as emotional factors, habits, and different sources
   of social influence. Our results (N = 238) suggest that the model well
   predicts the use of the Internet at work for non-work purposes. Our
   results shed new light on the influence of habit, affect, role, and
   self-concept in the use of the Internet. (c) 2013 Elsevier B.V. All
   rights reserved.}},
