@article{ ISI:000374607400016,
Author = {Priefer, Carmen and Joerissen, Juliane and Braeutigam, Klaus-Rainer},
Title = {{Food waste prevention in Europe - A cause-driven approach to identify
   the most relevant leverage points for action}},
