@article{ ISI:000366919200002,
Author = {Alvarez-Castano, Luz Stella and Goez-Rueda, Juan Diego and Diaz-Garcia,
   Juliana and Quintero-Morales, Maria Teresa},
Title = {{Methodology for food and nutritional evaluation from a perspective of
   equity}},
