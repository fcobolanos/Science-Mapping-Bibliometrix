@article{ ISI:000396618200007,
Author = {Hashemi, Seyed Mahmood and He, Jingsha},
Title = {{LA-Based Approach for IoT Security}},
Journal = {{JOURNAL OF ROBOTICS NETWORKING AND ARTIFICIAL LIFE}},
Year = {{2017}},
Volume = {{3}},
Number = {{4}},
Pages = {{240-248}},
Month = {{MAR}},
Abstract = {{The most important aspect in IoT is security. The provision of security
   in IoT systems is the responsibility of a trust management mechanism.
   However a trust management mechanism comprises a number of components,
   of which secure routing is vital among them. There are a number of
   effective parameters in secure routing which have been considered in the
   presented multi-objective optimization model. In this paper,
   MultiObjective Learning Automata (MOLA) was used to solve secure routing
   problem which can simultaneously optimize all parameters. There exist
   three methods of training LA, and the results of the different methods
   were compared in this study. The proposed approach can be used with both
   administrator and users because their requirements are considered in a
   model and it is quite easy for administrator and users to comprehend.}},
