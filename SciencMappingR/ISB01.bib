Author = {Bala, Kanchan},
Title = {{A Survey of Black Hole Detection Policies in Mobile Ad Hoc Networks}},
Journal = {{INTERNATIONAL JOURNAL OF FUTURE GENERATION COMMUNICATION AND NETWORKING}},
