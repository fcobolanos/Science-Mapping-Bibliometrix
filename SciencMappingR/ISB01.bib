@article{ ISI:000343352500007,
Author = {Naeem, Ensherah A. and Abd Elnaby, Mustafa M. and Soliman, Naglaa F. and
   Abbas, Alaa M. and Faragallah, Osama S. and Semary, Noura and Hadhoud,
   Mohiy M. and Alshebeili, Saleh A. and Abd El-Samie, Fathi E.},
Title = {{Efficient implementation of chaotic image encryption in transform
   domains}},
Journal = {{JOURNAL OF SYSTEMS AND SOFTWARE}},
Year = {{2014}},
Volume = {{97}},
Pages = {{118-127}},
Month = {{NOV}},
Abstract = {{The primary goal of this paper is security management in data image
   transmission and storage. Because of the increased use of images in
   industrial operations, it is necessary to protect the secret data of the
   image against unauthorized access. In this paper, we introduce a novel
   approach for image encryption based on employing a cyclic shift and the
   2-D chaotic Baker map in different transform domains. The Integer
   Wavelet Transform (IWT), the Discrete Wavelet Transform (DWT), and the
   Discrete Cosine Transform (DCT) are exploited in the proposed encryption
   approach. The characteristics of the transform domains are studied and
   used to carry out the chaotic encryption. A comparison study between the
   transform-domain encryption approaches in the presence of attacks shows
   the superiority of encryption in the DWT domain. (C) 2014 Elsevier Inc.
   All rights reserved.}},
