@article{ ISI:000372325300019,
Author = {Kobayashi, Kei and Totani, Yosuke and Utsu, Keisuke and Ishii, Hiroshi},
Title = {{Achieving secure communication over MANET using secret sharing schemes}},
Journal = {{JOURNAL OF SUPERCOMPUTING}},
Year = {{2016}},
Volume = {{72}},
Number = {{3}},
Pages = {{1215-1225}},
Month = {{MAR}},
Abstract = {{A mobile ad hoc network (MANET) has been studied in order to achieve
   network capability in disaster situations. However, MANET has many
   issues to be addressed. One of the issues is security. For example,
   MANET is vulnerable to network sniffing because it uses wireless
   communication, and also existing security approaches are very difficult
   to apply to MANET due to the absence of a trusted third party. In this
   paper, we propose a novel communication method that cannot easily be
   subjected to network sniffing, thereby addressing the issue of security.
   In addition, we evaluate the proposed method using a network simulator
   and show its effectiveness.}},
