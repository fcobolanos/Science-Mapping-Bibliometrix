@article{ ISI:000345883400003,
Author = {Butler, M. J.},
Title = {{Towards online security: Key drivers of poor user behaviour and
   recommendations for appropriate interventions}},
Journal = {{SOUTH AFRICAN JOURNAL OF BUSINESS MANAGEMENT}},
