@article{ ISI:000390579300049,
Author = {Kahouei, Mehdi and Zadeh, Jamileh Mahdi and Abbasi, Zainab},
Title = {{THREATS AND SOLUTIONS FOR THE SECURITY OF ELECTRONIC PATIENT RECORD
   (EPR) IN A DEVELOPING COUNTRY}},
Journal = {{IIOAB JOURNAL}},
Year = {{2016}},
