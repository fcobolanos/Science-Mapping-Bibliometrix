@article{ ISI:000431413600007,
Author = {Hanus, Bartlomiej and Windsor, John C. and Wu, Yu},
Title = {{Definition and Multidimensionality of Security Awareness: Close
   Encounters of the Second Order}},
Journal = {{DATA BASE FOR ADVANCES IN INFORMATION SYSTEMS}},
