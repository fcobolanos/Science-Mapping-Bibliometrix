DOI = {{10.1016/j.ijggc.2016.08.023}},
ISSN = {{1750-5836}},
EISSN = {{1878-0148}},
Keywords = {{Multi-criteria decision analysis; Life cycle assessment; Sustainability
   assessment; Power generation; Carbon capture and storage; Switzerland}},
