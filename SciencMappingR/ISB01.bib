@article{ ISI:000457158400001,
Author = {Madi, Taous and Jarraya, Yosr and Alimohammadifar, Amir and Majumdar,
   Suryadipta and Wang, Yushun and Pourzandi, Makan and Wang, Lingyu and
   Debbabi, Mourad},
Title = {{ISOTOP: Auditing Virtual Networks Isolation Across Cloud Layers in
   OpenStack}},
Journal = {{ACM TRANSACTIONS ON PRIVACY AND SECURITY}},
Year = {{2019}},
Volume = {{22}},
