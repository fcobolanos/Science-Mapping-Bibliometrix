@article{ ISI:000387979100005,
Author = {Priya, J. Sathya and Rajagopalan, S. P. and Ramakrishnan, M.},
Title = {{Medical Cyber Physical System Security-Mitigating Attacks Using Trust
   Model}},
Journal = {{JOURNAL OF MEDICAL IMAGING AND HEALTH INFORMATICS}},
Year = {{2016}},
Volume = {{6}},
Number = {{7}},
Pages = {{1572-1575}},
Month = {{NOV}},
Abstract = {{Cyber-Physical Systems (CPS) has attracted researchers from
   academic/industry as it is viewed as the next computing revolution. CPS
   has applications in domains like data-centers, health management,
   power-grids, vehicular management and physical infrastructure. Many
   issues have to be addressed to make each CPS system viable like managing
   cyber/physical interactions, interoperability, ensuring safety,
   energy-efficiency and sustainability. An important aspect to be
   considered in CPS' early development is security. This paper proposes
   energy aware security measures for a Medical CPS (MCPS) using trust to
   detect and mitigate Denial of Service attack. The new routing scheme
   considers packet deadline, energy and intermediate nodes congestion to
   deliver real-time traffic using trust. Extensive simulation show
   improved packet delivery and reduced end to end delay.}},
